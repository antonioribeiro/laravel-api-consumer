            "role": "Developer"
        },
        {
            "name": "Antonio Ribeiro",
            "email": "acr@antoniocarlosribeiro.com",
            "homepage": "https://antoniocarlosribeiro.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "kitetail/zttp": "^0.6.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|6.*|7.*|8.*"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BlackBits\\ApiConsumer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlackBits\\ApiConsumer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackBits\\ApiConsumer\\ApiConsumerServiceProvider"
            ]
        }
    }
}
